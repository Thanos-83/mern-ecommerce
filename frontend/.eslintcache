);\r\n    fileReader.onloadend = () => {\r\n      // setImageSelected(fileReader.result);\r\n      setProductData({ ...productData, image: fileReader.result });\r\n    };\r\n  };\r\n  return (\r\n    <div className='addProduct'>\r\n      <div className='snackbar'>\r\n        <Snackbar\r\n          open={open}\r\n          autoHideDuration={2000}\r\n          onClose={handleClose}\r\n          anchorOrigin={{\r\n            horizontal: 'right',\r\n            vertical: 'top',\r\n          }}>\r\n          <Alert variant='filled' onClose={handleClose} severity='success'>\r\n            Product Added Successfully!\r\n          </Alert>\r\n        </Snackbar>\r\n      </div>\r\n      <div className='addProduct__header'>\r\n        <h3>ADD NEW PRODUCT</h3>\r\n        <Button variant='outlined' size='small'>\r\n          <Link to='/dashboard/products'>Back to Products</Link>\r\n        </Button>\r\n      </div>\r\n      <div className='addProduct__form'>\r\n        <form\r\n          onSubmit={addProduct}\r\n          className='addProduct__form'\r\n          encType='multipart/form-data'>\r\n          <InputLabel error={0 ? true : false}>Product Name</InputLabel>\r\n          <TextField\r\n            value={productData.name}\r\n            variant='outlined'\r\n            required\r\n            id='name'\r\n            shrink='true'\r\n            autoFocus={true}\r\n            size='small'\r\n            // label='Required'\r\n            helperText={`${0 ? 'Incorrect entry.' : ''}`}\r\n            error={0 ? true : false}\r\n            onChange={(e) =>\r\n              setProductData({ ...productData, name: e.target.value })\r\n            }\r\n          />\r\n          <InputLabel>Product Category</InputLabel>\r\n          <Autocomplete\r\n            options={categories}\r\n            getOptionLabel={(option) => option.name}\r\n            fullWidth='true'\r\n            autoSelect='true'\r\n            value={productData.category}\r\n            onChange={(event, newValue) => {\r\n              // setValue(newValue);\r\n              if (newValue) {\r\n                setProductData({ ...productData, category: newValue.name });\r\n              } else {\r\n                setProductData({ ...productData, category: '' });\r\n              }\r\n            }}\r\n            inputValue={productData.category}\r\n            renderInput={(params) => (\r\n              <TextField\r\n                {...params}\r\n                required\r\n                shrink='true'\r\n                id='category'\r\n                // label='Required'\r\n                variant='outlined'\r\n              />\r\n            )}\r\n          />\r\n\r\n          <InputLabel>Brand</InputLabel>\r\n          <TextField\r\n            value={productData.brand}\r\n            required\r\n            id='brand'\r\n            // label='Required'\r\n            variant='outlined'\r\n            onChange={(e) =>\r\n              setProductData({ ...productData, brand: e.target.value })\r\n            }\r\n          />\r\n\r\n          <InputLabel>Description</InputLabel>\r\n          <TextField\r\n            value={productData.description}\r\n            required\r\n            id='description'\r\n            // label='Required'\r\n            variant='outlined'\r\n            multiline={true}\r\n            rows={5}\r\n            onChange={(e) =>\r\n              setProductData({ ...productData, description: e.target.value })\r\n            }\r\n          />\r\n\r\n          <InputLabel>Price</InputLabel>\r\n          <TextField\r\n            value={productData.price}\r\n            required\r\n            error={0 ? true : false}\r\n            id='price'\r\n            type='number'\r\n            // label='Required'\r\n            variant='outlined'\r\n            onChange={(e) =>\r\n              setProductData({ ...productData, price: e.target.value })\r\n            }\r\n          />\r\n\r\n          <InputLabel>Stock Quantity</InputLabel>\r\n          <TextField\r\n            value={productData.countInStock}\r\n            required\r\n            error={0 ? true : false}\r\n            id='quantity'\r\n            type='number'\r\n            // label='Required'\r\n            variant='outlined'\r\n            onChange={(e) =>\r\n              setProductData({ ...productData, countInStock: e.target.value })\r\n            }\r\n          />\r\n          <div className='product__images'>\r\n            <CloudUpload />\r\n            <InputLabel htmlFor='image'>\r\n              Upload Image\r\n              <span>(click here...)</span>\r\n            </InputLabel>\r\n\r\n            <input\r\n              type='file'\r\n              name='image'\r\n              id='image'\r\n              hidden={true}\r\n              placeholder='Featured Image'\r\n              onChange={uploadFileHandler}\r\n            />\r\n          </div>\r\n          <div className='images'>\r\n            <h3>here will be displied all images</h3>\r\n            {productData.image ? (\r\n              // <img src={`/${productData.image}`} alt='' />\r\n              <img src={productData.image} alt='selected' />\r\n            ) : (\r\n              'No files yet'\r\n            )}\r\n            <div\r\n              onClick={(e) => setProductData({ ...productData, image: '' })}\r\n              style={{ cursor: 'pointer' }}>\r\n              Delete image\r\n            </div>\r\n          </div>\r\n          <button\r\n            // onClick={addProduct}\r\n            type='submit'\r\n            className='w-full sm:w-3/4 md:w-1/2 lg:w-1/4 py-2 border border-orange-400 bg-orange-600 rounded text-slate-200 mt-6'>\r\n            Add Product\r\n          </button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default AddProduct;\r\n","C:\\Users\\Thanos\\Desktop\\web development projects\\React Projects\\ecommemrce-mern\\frontend\\src\\admin-dashboard\\admin-components\\main\\categories\\Produ